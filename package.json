{
	"name":"test",
	"description": "Best project ever!",
	"main": "lib",
	"version": "0.1.0",
	"homepage": "https://github.com/DoubleDome",
	"author": {
		"name": "Alex Toledo"
	},
    "paths":{
        "source":{
            "less":"<%= data.paths.dir.build %>less/**/*.less",
            "project":"<%= data.paths.dir.build %>less/project.less",
            "css":"<%= data.paths.dir.build %>css/**/*.css",
            "js":"<%= data.paths.dir.build %>lib/**/*.js"
        },
        "dir":{
            "build":"build/",
            "release":"release/",
            "tag":"tags/tag-<%= data.version %>/"
        },
        "output":{
            "project":"<%= data.paths.dir.build %>css/global.css",
            "tag":"tags/tag-<%= data.version %>.zip"
        },
        "excludes":[
            "<%= data.paths.dir.release %>test",
            "<%= data.paths.dir.release %>dist",
            "<%= data.paths.dir.release %>*.styl",
            "<%= data.paths.dir.release %>**/*.sass",
            "<%= data.paths.dir.release %>**/less",
            "<%= data.paths.dir.release %>**/*.coffee",
            "<%= data.paths.dir.release %>*.DS_Store"
        ]
    },
    "dependencies":{
       "grunt-recess":"*",
       "grunt-contrib-copy":"*",
       "grunt-contrib-clean":"*",
       "grunt-contrib-compress":"*",
       "grunt-less":"*"
    }

}